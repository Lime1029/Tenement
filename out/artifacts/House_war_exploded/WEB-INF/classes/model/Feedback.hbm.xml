<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.Feedback" table="feedback" schema="tenement">
        <id name="feedbackId" column="feedback_id"/>
        <property name="feedback" column="feedback"/>
        <property name="feedbackIime" column="feedback_iime"/>
        <property name="authorId" column="author_id"/>
    </class>
</hibernate-mapping>