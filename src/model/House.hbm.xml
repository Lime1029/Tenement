<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.House" table="house" schema="tenement">
        <id name="houseId" column="house_id"/>
        <property name="landlordId" column="landlord_id"/>
        <property name="plotId" column="plot_id"/>
        <property name="address" column="address"/>
        <property name="rent" column="rent"/>
        <property name="state" column="state"/>
        <property name="rentalType" column="rentalType"/>
        <property name="orientation" column="orientation"/>
        <property name="houseType" column="house_type"/>
        <property name="floor" column="floor"/>
        <property name="area" column="area"/>
        <property name="rentTerm" column="rent_term"/>
        <property name="houseDescription" column="house_description"/>
        <property name="releaseTime" column="release_time"/>
    </class>
</hibernate-mapping>